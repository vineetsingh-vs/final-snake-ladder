  "private": true,
  "dependencies": {
    "@abandonware/bleno": "^0.5.1-4",
    "@abandonware/noble": "^1.9.2-21",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
